{
  "name": "qwiksale-frontend",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev --turbopack",
    "build": "prisma generate && next build",
    "start": "next start",
    "postinstall": "prisma generate",
    "lint": "echo \"(lint disabled)\"",
    "lint:fix": "echo \"(lint disabled)\"",
    "typecheck": "tsc --noEmit",
    "analyze": "ANALYZE=true next build",
    "prisma:generate": "prisma generate",
    "prisma:migrate": "prisma migrate dev",
    "prisma:deploy": "prisma migrate deploy",
    "prisma:studio": "prisma studio",
    "db:seed": "prisma db seed",
    "seed:demo": "cross-env SEED_DEMO=1 prisma db seed",
    "seed:reset": "cross-env SEED_RESET=1 SEED_RESET_ALL=1 prisma db seed",
    "seed:clear-demo": "cross-env SEED_PURGE_DEMO=1 prisma db seed",
    "db:seed:120": "cross-env SEED_RESET=1 SEED_RESET_ALL=1 SEED_DEMO=1 prisma db seed",
    "db:purge": "tsx scripts/purge.ts",
    "backup": "tsx scripts/backup.ts",
    "health:local": "cross-env NODE_ENV=production curl -s http://localhost:3000/api/health || echo \"health check requires curl\"",
    "test": "vitest --run",
    "test:unit": "vitest --run --dir tests/unit",
    "test:api": "vitest --run --dir tests/integration",
    "test:e2e": "playwright test",
    "test:e2e:ui": "playwright test --ui",
    "test:install-browsers": "playwright install --with-deps",
    "ci": "npm run typecheck && npm run test:unit && npm run build && npm run test:e2e",
    "smoke:db": "tsx scripts/quick-check.ts",
    "smoke:feed": "tsx scripts/smoke-home-feed.ts",
    "prepr": "pnpm typecheck && pnpm smoke:feed && pnpm smoke:db",
    "prepr:full": "pnpm typecheck && pnpm build && pnpm smoke:feed && pnpm smoke:db"
  },
  "engines": {
    "node": "20.x",
    "npm": "please-use-pnpm",
    "yarn": "please-use-pnpm"
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.888.0",
    "@next-auth/prisma-adapter": "^1.0.7",
    "@prisma/client": "^6.16.3",
    "@sentry/nextjs": "^10.10.0",
    "@upstash/ratelimit": "^2.0.6",
    "@upstash/redis": "^1.35.3",
    "@vercel/analytics": "^1.5.0",
    "bcryptjs": "^3.0.2",
    "cloudinary": "^2.7.0",
    "next": "15.5.0",
    "next-auth": "^4.24.11",
    "nodemailer": "^6.10.1",
    "pino": "^9.5.0",
    "posthog-js": "^1.261.6",
    "react": "19.1.0",
    "react-dom": "19.1.0",
    "react-hot-toast": "^2.6.0",
    "resend": "^6.0.2",
    "sharp": "^0.33.4"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3",
    "@eslint/js": "^8.57.0",
    "@next/bundle-analyzer": "^15.5.3",
    "@next/eslint-plugin-next": "^15.5.2",
    "@playwright/test": "^1.55.0",
    "@rushstack/eslint-patch": "^1.12.0",
    "@tailwindcss/aspect-ratio": "^0.4.2",
    "@tailwindcss/container-queries": "^0.1.1",
    "@tailwindcss/forms": "^0.5.10",
    "@tailwindcss/typography": "^0.5.16",
    "@types/africastalking": "^0.6.2",
    "@types/node": "^20",
    "@types/nodemailer": "^7.0.1",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "@typescript-eslint/eslint-plugin": "^7.18.0",
    "@typescript-eslint/parser": "^7.18.0",
    "@vitest/coverage-v8": "^3.2.4",
    "autoprefixer": "^10.4.21",
    "client-only": "0.0.1",
    "cross-env": "^10.0.0",
    "cssnano": "^6.0.1",
    "dotenv": "^17.2.1",
    "eslint": "^8.57.0",
    "eslint-config-next": "^15.5.2",
    "eslint-import-resolver-typescript": "^3.10.1",
    "eslint-plugin-import": "^2.32.0",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-tailwindcss": "^3.18.2",
    "eslint-plugin-unused-imports": "^4.2.0",
    "globals": "^16.3.0",
    "pino-pretty": "^11.2.2",
    "playwright": "^1.55.0",
    "postcss": "^8.5.6",
    "postcss-import": "^16.1.0",
    "prisma": "^6.16.3",
    "server-only": "0.0.1",
    "tailwindcss": "^3.4.14",
    "tailwindcss-animate": "^1.0.7",
    "tsx": "^4.20.5",
    "typescript": "^5.9.2",
    "typescript-eslint": "^7.18.0",
    "vite": "^5.4.20",
    "vite-tsconfig-paths": "^5.1.4",
    "vitest": "^3.2.4"
  },
  "packageManager": "pnpm@10.16.0"
}
